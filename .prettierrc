{
	"overrides": [
		{
			"files": ".prettierrc",
			"options": {
				"parser": "json",
				"useTabs": true,
				"trailingComma": "none"
			}
		},
		{
			"files": "*.js",
			"options": {
				"parser": "babel",
				"printWidth": 120,
				"tabWidth": 4
			}
		},
		{
			"files": "*.json",
			"options": {
				"parser": "json",
				"useTabs": true,
				"trailingComma": "none"
			}
		}
	]
}
